                                    w.Material =
                                        o["Visuals"]["Others"]["Dropped_Weapons_Material"] == "Custom" and
                                        Enum.Material.SmoothPlastic or
                                        o["Visuals"]["Others"]["Dropped_Weapons_Material"] == "Flat" and
