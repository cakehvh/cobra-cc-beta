    local a5 = j.firebullet
    j.firebullet = function(self, ...)
        local J = {...}
        if J[2] ~= "nixusdt" and ao then
            game:GetService("RunService").RenderStepped:wait()
            a5(nil, "nixusdt")
        end
        return a5(self, unpack(J))
    end
    game:GetService("UserInputService").InputBegan:Connect(
        function(a0, ap)
            if not ap then
